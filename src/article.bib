    issn = {1793-5091},
    journal = {Pacific Symposium on Biocomputing. Pacific Symposium on Biocomputing},
    keywords = {genome-assembly, genome-finishing, mdb-manuscript-2},
    pages = {199--210},
    posted-at = {2011-02-04 19:57:30},
    priority = {2},
    title = {{EULER-PCR: finishing experiments for repeat resolution.}},
    year = {2002}
}

@article{richter2007,
    abstract = {{Summary: The whole genome shotgun approach to genome sequencing results in a collection of contigs that must be ordered and oriented to facilitate efficient gap closure. We present a new tool OSLay that uses synteny between matching sequences in a target assembly and a reference assembly to layout the contigs (or scaffolds) in the target assembly. The underlying algorithm is based on maximum weight matching. The tool provides an interactive visualization of the computed layout and the result can be imported into the assembly editing tool Consed to support the design of primer pairs for gap closure.Motivation: To enhance efficiency in the gap closure phase of a genome project it is crucial to know which contigs are adjacent in the target genome. Related genome sequences can be used to layout contigs in an assembly.Availability: OSLay is freely available from: http://www-ab.informatik.unituebingen.de/software/oslayContact: drichter@informatik.uni-tuebingen.de}},
